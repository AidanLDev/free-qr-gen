    "breakpoint-hooks": "^1.0.2",
    "date-fns": "^4.1.0",
    "next": "16.0.3",
    "next-sitemap": "^4.2.3",
    "nodemailer": "^6.10.1",
    "qrcode.react": "^4.2.0",
    "react": "19.2.0",
    "react-color": "^2.19.3",
    "react-dom": "19.2.0",
    "react-icons": "^5.5.0",
    "react-toastify": "^10.0.6",
    "react-tooltip": "^5.30.0",
    "sst": "^3.17.23"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "^20.19.25",
